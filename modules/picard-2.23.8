#%Module1.0#####################################################################
##
## Picard 2.23.8 modulefile
##

## Module file logging
#source /usr/local/etc/module_logging.tcl
##

#This version of Picard needs Java 1.6
#module load apps/java/1.6

proc ModulesHelp { } {
        puts stderr "\tThis module sets up the environment for picard, version 2.23.8"
}

#set version 2.23.8
set PICARD_DIR /opt/sw/bioinfo-tools/sources/picard/build/libs

module-whatis   "Makes Picard 2.23.8 available"

prepend-path PICARDHOME $PICARD_DIR
